{
  "expo": {
    "name": "Football IQ",
    "slug": "football-iq",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/footballiq-icon.png",
    "scheme": "footballiq",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F172A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.charliearlie.footballiq.app",
      "icon": "./assets/images/footballiq-icon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/footballiq-icon.png",
        "backgroundColor": "#0F172A"
      },
      "package": "com.charliearlie.footballiq.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store",
      "expo-sqlite",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-9426782115883407~1712062487",
          "iosAppId": "ca-app-pub-9426782115883407~8797195643"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
